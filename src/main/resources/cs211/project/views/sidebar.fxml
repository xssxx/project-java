            <HBox alignment="CENTER" onMouseClicked="#onMyTeam" prefHeight="50.0" prefWidth="125.0">
               <children>
                  <Label text="My team" textFill="#767990">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                     <font>
                        <Font size="17.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Label>
               </children>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </HBox>
         </children>
      </VBox>
      <VBox alignment="BOTTOM_CENTER" prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox fx:id="adminBox" alignment="CENTER" onMouseClicked="#onClickAdmin" prefHeight="35.0" prefWidth="125.0">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <children>
                  <Label text="Admin" textFill="#767990">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER" onMouseClicked="#onClickUser" prefHeight="35.0" prefWidth="125.0">
               <children>
                  <Label text="My Account" textFill="#767990">
