            <HBox alignment="CENTER" onMouseClicked="#onEditList" prefHeight="50.0" prefWidth="125.0">
               <children>
                  <Label text="Edit" textFill="#767990">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                     <font>
                        <Font size="17.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Label>
               </children>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </HBox>
            <HBox alignment="CENTER" onMouseClicked="#onHistory" prefHeight="50.0" prefWidth="125.0">
               <children>
                  <Label text="History" textFill="#767990">
